<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
		 version="3.0">

	<description>zats-test-app</description>
	<display-name>zats-test-app</display-name>

	<context-param>
		<param-name>contextConfigLocation</param-name>
		<param-value>zk.springboot.Application</param-value>
	</context-param>

	<listener>
		<listener-class>org.zkoss.zkspringboot.zats.ZatsSpringBootContextLoaderListener</listener-class>
	</listener>
	<listener>
		<listener-class>org.zkoss.zk.ui.http.HttpSessionListener</listener-class>
	</listener>
</web-app>
